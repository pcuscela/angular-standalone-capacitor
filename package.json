{
  "name": "angular-standalone-capacitor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration development",
    "build:test": "ng build --configuration test",
    "build:prod": "ng build --configuration production && node postbuild.js",
    "format": "prettier --log-level warn --write \"src/app/**/*.{html,ts}\"",
    "watch": "ng build --watch --configuration development",
    "extract-i18n": "ngx-translate-extract --input ./src --output ./src/assets/i18n/it.json ./src/assets/i18n/en.json --clean --sort --format namespaced-json",
    "-------": "--------",
    "app:build": "ng build --configuration development && npx cap sync",
    "app:build:prod": "ng build --configuration production && npx cap sync && node postbuild.js",
    "app:copy": "ng build --configuration development && npx cap copy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@capacitor/android": "^6.1.0",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/core": "latest",
    "@capacitor/ios": "^6.1.0",
    "@colsen1991/ngx-translate-extract-marker": "^2.0.8",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ng-select/ng-select": "^12.0.7",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/context-zone": "^1.24.1",
    "@opentelemetry/exporter-logs-otlp-http": "^0.51.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.51.1",
    "@opentelemetry/instrumentation-document-load": "^0.38.0",
    "@opentelemetry/instrumentation-xml-http-request": "^0.51.1",
    "@opentelemetry/sdk-logs": "^0.51.1",
    "@opentelemetry/sdk-trace-web": "^1.24.1",
    "@popperjs/core": "^2.11.8",
    "@sentry/angular": "^8.5.0",
    "@sentry/cli": "^2.32.1",
    "@vendure/ngx-translate-extract": "^9.1.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "ng-block-ui": "^4.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.9",
    "@angular/cli": "^17.0.9",
    "@angular/compiler-cli": "^17.0.0",
    "@angular/localize": "^17.0.0",
    "@capacitor/cli": "latest",
    "prettier": "^3.2.4",
    "typescript": "~5.2.2"
  }
}